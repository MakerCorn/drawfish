{
  "name": "drawfish",
  "version": "1.0.0",
  "description": "AI-powered Mermaid diagram generator supporting multiple LLM providers",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "node --watch server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run lint && npm run format:check",
    "precommit": "npm run validate"
  },
  "keywords": [
    "mermaid",
    "diagrams",
    "ai",
    "ollama",
    "azure",
    "bedrock",
    "vertex"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@google-cloud/vertexai": "^1.0.0",
    "@azure/openai": "^1.0.0-beta.11",
    "axios": "^1.6.2",
    "puppeteer": "^21.6.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  }
}
