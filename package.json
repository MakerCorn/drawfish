{
  "name": "drawfish",
  "version": "1.2.0",
  "description": "AI-powered Mermaid diagram generator supporting multiple LLM providers",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "node --watch server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run lint && npm run format:check",
    "precommit": "npm run validate"
  },
  "keywords": [
    "mermaid",
    "diagrams",
    "ai",
    "ollama",
    "azure",
    "bedrock",
    "vertex"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@google-cloud/vertexai": "^1.0.0",
    "axios": "1.13.1",
    "cors": "^2.8.5",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "openai": "^6.7.0",
    "puppeteer": "^24.26.1"
  },
  "devDependencies": {
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "prettier": "3.6.2"
  }
}
